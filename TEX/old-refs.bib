number = {5-6},
pages = {1051--1066},
shorttitle = {Triggering of Seismicity by Pore-pressure Perturbations},
title = {{Triggering of Seismicity by Pore-pressure Perturbations: Permeability-related Signatures of the Phenomenon}},
url = {http://link.springer.com/article/10.1007/PL00012560},
volume = {160},
year = {2003}
