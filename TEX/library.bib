keywords = {Bioretention,Construction,LID,Maintenance,Media depth,Runoff,SUDS,Stormwater,WSUD},
number = {2},
pages = {361--367},
pmid = {22233916},
title = {{Improving bioretention/biofiltration performance with restorative maintenance}},
