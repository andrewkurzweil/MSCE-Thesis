journal = {Journal of Sustainable Water in the Built Environment},
keywords = {Author keywords: Low impact development (LID),Bioinfiltration,Dynamic modeling,Infiltration trench,Rain garden,Saturated hydraulic conductivity,Storm Water Management Model (SWMM),Stormwater control measure (SCM)},
month = {may},
